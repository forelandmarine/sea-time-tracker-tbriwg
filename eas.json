
{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "bundleIdentifier": "com.forelandmarine.seatimetracker.dev"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.forelandmarine.seatimetracker.preview"
      },
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "bundleIdentifier": "com.forelandmarine.seatimetracker"
      },
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
