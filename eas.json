
{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "bundleIdentifier": "com.forelandmarine.ais-seatime-tracker.dev",
        "appleTeamId": "43GZCFFPR9"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "bundleIdentifier": "com.forelandmarine.ais-seatime-tracker.preview",
        "appleTeamId": "43GZCFFPR9"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "bundleIdentifier": "com.forelandmarine.ais-seatime-tracker",
        "appleTeamId": "43GZCFFPR9"
      },
      "android": {
        "buildType": "aab"
      },
      "channel": "production",
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleTeamId": "43GZCFFPR9"
      }
    }
  }
}
